    <section id="cart_items">
		<div class="container">
			<div class="table-responsive cart_info">
				<table class="table table-condensed">
					<thead>
						<tr class="cart_menu">
							<td class="image">Item</td>
							<td class="description"></td>
							<td class="price">Price</td>
							<td class="quantity">Quantity</td>
							<td class="total">Total</td>
							<td></td>
						</tr>
					</thead>
					<tbody>
						
						<% var i,sum=0,price %>

							<% for (i = 0; i < item.length; i++) { %>
							  <% var x=quantity[i],month,week %>
							 
							  
								
							<%	if(x>=30){ %>
							<%		month=Math.floor(x/30); %>
							<%	x-=(30*month);%>
							<%		if(x>=7){ %>
							<%		week=Math.floor(x/7); %>
							<%		x-=(7*week);%>
							<%			if(x==0){  %>
							        <% price= (item[i].price_month * month)+(item[i].price_week * week)  %>
							        
											
							<%			}else{  %>
							      <% price=(item[i].price_month * month)+(item[i].price_week * week)+(item[i].price_day * x)   %>
										
							<%			} %>
							<%		}else{ %>
							      <% price=(item[i].price_month * month)   %>
										<%=price %>
							<%	} %>
							<%	}else if(x>=7){ %>
							<%			week=Math.floor(x/7); %>
							<%			x-=(7*week); %>
							<%			if(x==0){ %>
							        <% price= (item[i].price_week * week)  %>
											<%=price %>
							<%			}else{ %>
							        <% price= (item[i].price_week * week)+(item[i].price_day * x)  %>
											 
							<%			} %>
							<%	}else{ %>
							    <% price=(item[i].price_day * x)   %>
									
							<%	} %>
							  
							  <%sum=sum+price; %>
						<tr>
							
							
								<td class="cart_product">
									<a href="/item/<%=item[i]._id%>"><img src="/uploads/<%=item[i].image%>" alt=""></a>
								</td>
								<td class="cart_description">
									<h4><a href="/item/<%=item[i]._id%>"><%=item[i].name%></a></h4>
									<p>Web ID: 1089772</p>
								</td>
								<td class="cart_price">
									<p><%=item[i].price_day%></p>
								</td>
								<td class="cart_quantity">
									<div class="cart_quantity_button">
										<a class="cart_quantity_up" href="/cart/<%=cart._id%>/<%=i%>/up"> + </a>
										<input class="cart_quantity_input" type="text" name="quantity" value="<%=quantity[i]%>" autocomplete="off" size="2">
										<a class="cart_quantity_down" href="/cart/<%=cart._id%>/<%=i%>/down"> - </a>
									</div>
								</td>
								<td class="cart_total">
									<p class="cart_total_price"><%=price%></p>
								</td>
								<td class="cart_delete">
									<a class="cart_quantity_delete" href="/cart/<%=item[i]._id%>/remove"><i class="fa fa-times"></i></a>
								</td>
						</tr>
						 
						
					<%}%>

						
						
						

					</tbody>
				</table>
			</div>
		</div>
	</section> <!--/#cart_items-->

